{
	"tasks": {
		"dev": "deno run --watch -P=dev --env-file=.env main.ts",
		"start": "deno run -P --env-file=.env main.ts",
		"compile": "deno compile -P main.ts",
		"compile-aarch64": "deno compile -P --target=aarch64-unknown-linux-gnu main.ts",
	},
	"permissions": {
		"default": {
			"env": true,
			"net": true,
			"ffi": true,
			"read": true,
			"write": true,
		},
		"dev": {
			"env": true,
			"net": true,
			"ffi": true,
			"read": true,
			"write": true,
		},
	},
	"lint": {
		"rules": {
			"exclude": ["no-explicit-any"],
		},
	},
	"imports": {
		"@db/sqlite": "jsr:@db/sqlite@^0.12.0",
		"@mathis/turnstile-verify": "jsr:@mathis/turnstile-verify@^1.2.0",
		"@msgpack/msgpack": "npm:@msgpack/msgpack@^3.1.2",
		"@openai/openai": "jsr:@openai/openai@^5.20.1",
		"@std/fs": "jsr:@std/fs@^1.0.19",
		"@types/cors": "npm:@types/cors@^2.8.19",
		"@types/express": "npm:@types/express@^5.0.3",
		"@types/node": "npm:@types/node@^24.3.1",
		"@types/pako": "npm:@types/pako@^2.0.4",
		"cors": "npm:cors@^2.8.5",
		"discord.js": "npm:discord.js@^14.25.1",
		"express-msgpack": "npm:express-msgpack@^6.0.0",
		"express-session": "npm:express-session@^1.18.0",
		"express": "npm:express@^5.1.0",
		"passport": "npm:passport@^0.7.0",
		"passport-github2": "npm:passport-github2@^0.1.12",
		"pako": "npm:pako@^2.1.0",
	},
	"compilerOptions": {
		"strict": true,
		"strictNullChecks": true,
	},
}
